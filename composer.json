{
  "name": "weerd/veritas-logs",
  "description": "Laravel package for helping read the log file during development.",
  "keywords": ["laravel", "logs"],
  "license": "MIT",
  "authors": [
    {
      "name": "Diego Lorenzo",
      "email": "diego@diegolorenzo.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Weerd\\VeritasLogs\\": "src/"
    }
  },
  "require": {
    "laravel/framework": "^5.4"
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Weerd\\VeritasLogsTests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "symfony/var-dumper": "^3.1"
  }
}
